	<div class="hub-row-main hub-row-left">
		<div class="hub-row-title">
			<p>{{wca.indicator1}}</p>
		</div>
		<div class="hub-row-container container" id="{{@key}}-hub-row-left">
			<div class="hub-square bordered-square col">
				<div>
					<p class="wca-indicator-title">WCA</p>
				</div>
				<div class="hub-square-indicators">
					<span class="wca-indicator-value">{{value1}}</span>
					<span class="wca-separator">/</span>
					<span class="wca-indicator-target">{{target1}}</span>
					<i class="wca-indicator-arrow {{getArrow arrow1}}"></i>
				</div>
				<div class="hub-square-indicators">
					<span class="wca-indicator-date">{{date1}}</span>
					<span class="wca-separator">=</span>
					<span class="wca-indicator-old">{{old1}}</span>
				</div>
				<div>
					<span>Likelihood</span>
					<span class="wca-indicator-likelihood">
						<div class="line {{getLikelihood likelihood1}}"></div>
					</span>
				</div>
