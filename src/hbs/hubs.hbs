<div class="scorecard-label">
    <h3 class="scorecard-title">WCA SCORECARDS (by Hub), {{Title}}</h3>
    <span class="scorecard-motto">{{motto}}</span>
</div>
<div class="scorecard-management container">
    <div class="management-title">
        <p>MANAGEMENT TEAM:</p>
    </div>
    <div class="management-element">
        <img src="/sites/pa/tools/v2/img/hubs/director.jpg">
        <div>
            <p>Lisandro Martin</p>
            <p>Regional Director</p>
        </div>
    </div>
    <div class="management-element">
        <img src="/sites/pa/tools/v2/img/hubs/west.jpg">
        <div>
            <p>Benoit Thierry</p>
            <p>Hub Head, West Africa</p>
        </div>
    </div>
    <div class="management-element">
        <img src="/sites/pa/tools/v2/img/hubs/coastal.jpg">
        <div>
            <p>Abdul Wahab Barry</p>
            <p>Hub Head, Coastal Africa</p>
        </div>
    </div>
    <div class="management-element">
        <img src="/sites/pa/tools/v2/img/hubs/central.jpg">
        <div>
            <p>Abdelhaq Hanafi</p>
            <p>Hub Head, Central Africa</p>
        </div>
    </div>
</div>
{{log this}}
{{#each data}}
{{log wca}}

<div class="scorecard-row">
